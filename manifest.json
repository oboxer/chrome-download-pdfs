{
  "manifest_version": 3,
  "name": "PDF Downloader",
  "version": "1.0",
  "description": "Download all PDFs from open tabs",
  "permissions": [
    "tabs",
    "downloads"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {}
}
